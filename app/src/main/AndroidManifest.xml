<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duanshu.h5.mobile.demo">

    <application
        android:name=".DuanshuApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ImmersiveActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <data
                    android:scheme="duanshu"
                    android:host="com.duanshu.h5.mobile"
                    android:path="/previewimage">

                </data>
            </intent-filter>
        </activity>

        <activity android:name=".BrowserActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <data
                    android:scheme="duanshu"
                    android:host="com.duanshu.h5.mobile"
                    android:path="/browser">

                </data>`
            </intent-filter>
        </activity>

        <activity android:name=".ViewPagerActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.VIEW"></action>
                <category android:name="android.intent.category.DEFAULT"></category>
                <data
                    android:scheme="duanshu"
                    android:host="com.duanshu.h5.mobile"
                    android:path="/previewpics">

                </data>
            </intent-filter>
        </activity>

        <meta-data android:name="com.duanshu.h5.mobile.APP_ID" android:value="dsb538je91521b942dg0"></meta-data>
        <meta-data android:name="com.duanshu.h5.mobile.APP_SECRET" android:value="qRMO2JDB19NWjBnm"></meta-data>

    </application>

</manifest>